<% content_for :modal_title do %>
  <%= t('select_asset') %>
<% end %>
<% content_for :modal_content do %>
  <%= render "currencies/list", currencies: Currency.swappable, identifier: params[:identifier] %>
<% end %>

<%= turbo_stream.append "modal-slot" do %>
  <%= render "view_modals/model", backdrop: 'static' %>
<% end %>

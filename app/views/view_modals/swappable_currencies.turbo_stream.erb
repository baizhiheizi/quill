<% content_for :modal_title do %>
  <%= t('select_asset') %>
<% end %>
<% content_for :modal_content do %>
  <%= render "currencies/list", currencies: SwapOrder::FOXSWAP_ENABLE ? Currency.swappable : Currency.pricable, identifier: params[:identifier] %>
<% end %>

<%= turbo_stream.append "modal-slot" do %>
  <%= render "view_modals/modal", backdrop: 'static' %>
<% end %>

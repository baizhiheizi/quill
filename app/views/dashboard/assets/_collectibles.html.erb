<%= turbo_frame_tag "my_assets_nft" do %>
  <% if @collectibles.blank? %>
    <%= render EmptyComponent.new text: t('no_record') %>
  <% else %>
    <div class="grid grid-cols-2 sm:grid-cols-3 lg:grid-cols-4 gap-4">
      <%= render partial: "dashboard/assets/collectible", collection: @collectibles, as: :collectible %>
    </div>
  <% end %>
<% end %>
